<?xml version="1.0" encoding="UTF-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置背景颜色-->
    <solid android:color="#ffffff"/>
    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"/>

    <!--设置圆角半径值-->
    <corners
        android:radius="5dp"/>

    <!--设置边界属性-->
    <stroke
        android:width="0.5dp"
        android:color="#cccccc"/>
</shape>