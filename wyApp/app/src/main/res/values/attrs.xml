<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="mylistdivider" format="reference"/>

    <declare-styleable name="GoogleCircleProgressView">
        <attr name="gcp_inner_radius" format="dimension"/>
        <attr name="gcp_background_color" format="color"/>
        <attr name="gcp_progress_color" format="color"/>
        <attr name="gcp_progress_stoke_width" format="dimension"/>
        <attr name="gcp_show_arrow" format="boolean"/>
        <attr name="gcp_enable_circle_background" format="boolean"/>
        <attr name="gcp_arrow_width" format="dimension"/>
        <attr name="gcp_arrow_height" format="dimension"/>

        <attr name="gcp_progress" format="integer"/>
        <attr name="gcp_max" format="integer"/>


        <attr name="gcp_progress_text_size" format="dimension"/>
        <attr name="gcp_progress_text_color" format="color"/>

        <!--<attr name="gcp_progress_text_offset" format="dimension"/>-->

        <attr name="gcp_progress_text_visibility" format="enum">
            <enum name="visible" value="0"/>
            <enum name="invisible" value="1"/>
        </attr>

    </declare-styleable>
    <!-- 飘星自定义属性 -->
    <declare-styleable name="live_heart_layout">
        <attr name="initX" format="dimension" />
        <attr name="initY" format="dimension" />
        <attr name="xRand" format="dimension" />
        <attr name="animLengthRand" format="dimension" />
        <attr name="xPointFactor" format="dimension" />
        <attr name="animLength" format="dimension" />
        <attr name="heart_width" format="dimension" />
        <attr name="heart_height" format="dimension" />
        <attr name="bezierFactor" format="integer" />
        <attr name="anim_duration" format="integer" />
    </declare-styleable>
	<!-- 标题模板自定义属性 -->
    <declare-styleable name="TemplateTitle">
        <!-- 标题文案 -->
        <attr name="titleText" format="string"/>
        <!-- 是否有back按钮 -->
        <attr name="canBack" format="boolean"/>
        <!-- back按钮文案 -->
        <attr name="backText" format="string"/>
        <!-- 右侧更多功能按钮图片 -->
        <attr name="moreImg" format="reference"/>
        <!-- 右侧更多功能按钮文字 -->
        <attr name="moreText" format="string"/>
    </declare-styleable>
</resources>